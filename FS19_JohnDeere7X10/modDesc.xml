    <actions>
		<action name="IC_SPACE" 		 category="VEHICLE"	axisType="HALF"/>
		<action name="IC_MOUSE" 		 category="VEHICLE"	axisType="HALF"/>	
		<action name="SET_TIRE_PRESSURE" category="VEHICLE" ignoreComboMask="false"/>
	</actions>
	
   <inputBinding>
   		<actionBinding action="IC_SPACE">
			<binding device="KB_MOUSE_DEFAULT" 	input="KEY_space" axisComponent="+" neutralInput="0" index="1"/>
		</actionBinding>
		
		<actionBinding action="IC_MOUSE">
			<binding device="KB_MOUSE_DEFAULT" input="MOUSE_BUTTON_LEFT" index="3"/>
		</actionBinding>
		
		<actionBinding action="SET_TIRE_PRESSURE">
			<binding device="KB_MOUSE_DEFAULT" input="KEY_k" axisComponent="+" neutralInput="0" index="1"/>
			<binding device="0_XINPUT_GAMEPAD" input="BUTTON_2" axisComponent="+" neutralInput="0" index="1"/>
		</actionBinding>
	</inputBinding>	
				
    <storeItems>
